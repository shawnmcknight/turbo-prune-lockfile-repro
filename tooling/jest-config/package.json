  "sideEffects": false,
  "exports": {
    "./nodejs": "./nodejs.js",
    "./nodejs.js": "./nodejs.js",
    "./react": "./react.js",
    "./react.js": "./react.js"
  },
  "devDependencies": {
    "@storis/eslint-config": "workspace:*",
    "@storis/tsconfig": "workspace:*",
